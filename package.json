  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "typeorm migration:run",
    "db:migrate:generate": "typeorm migration:generate",
    "db:migrate:revert": "typeorm migration:revert",
    "seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "lint:i18n": "eslint \"src/i18n/*.json\"",
    "generate:templates": "ts-node -r tsconfig-paths/register scripts/generate-template-types.ts",
    "validate:notification-manifests": "ts-node -r tsconfig-paths/register scripts/validate-notification-manifests.ts",
    "validate:notification-manifests:warn": "set VALIDATE_WARN_ONLY=true && ts-node -r tsconfig-paths/register scripts/validate-notification-manifests.ts",
    "validate:notification-listeners": "ts-node -r tsconfig-paths/register scripts/validate-notification-listeners.ts",
    "validate:templates": "ts-node -r tsconfig-paths/register scripts/validate-templates.ts",
    "validate:translations": "ts-node -r tsconfig-paths/register scripts/validate-translations.ts"
  },
